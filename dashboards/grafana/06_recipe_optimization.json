          },
          "format": 1,
          "queryType": "sql",
          "rawSql": "WITH gaps AS (\n  SELECT\n    'Temperature' as parameter,\n    avg(ref_temp_opt) as target_value,\n    avg(air_temp_internal) as actual_value,\n    '¬∞C' as unit\n  FROM vertiflow.basil_ultimate_realtime\n  WHERE timestamp > now() - INTERVAL 1 HOUR\n  UNION ALL\n  SELECT\n    'Humidity' as parameter,\n    avg(ref_humidity_opt) as target_value,\n    avg(air_humidity) as actual_value,\n    '%' as unit\n  FROM vertiflow.basil_ultimate_realtime\n  WHERE timestamp > now() - INTERVAL 1 HOUR\n  UNION ALL\n  SELECT\n    'Nitrogen (N)' as parameter,\n    avg(ref_n_target) as target_value,\n    avg(nutrient_n_total) as actual_value,\n    'ppm' as unit\n  FROM vertiflow.basil_ultimate_realtime\n  WHERE timestamp > now() - INTERVAL 1 HOUR\n  UNION ALL\n  SELECT\n    'Phosphorus (P)' as parameter,\n    avg(ref_p_target) as target_value,\n    avg(nutrient_p_phosphorus) as actual_value,\n    'ppm' as unit\n  FROM vertiflow.basil_ultimate_realtime\n  WHERE timestamp > now() - INTERVAL 1 HOUR\n  UNION ALL\n  SELECT\n    'Potassium (K)' as parameter,\n    avg(ref_k_target) as target_value,\n    avg(nutrient_k_potassium) as actual_value,\n    'ppm' as unit\n  FROM vertiflow.basil_ultimate_realtime\n  WHERE timestamp > now() - INTERVAL 1 HOUR\n  UNION ALL\n  SELECT\n    'Leaf Area Index' as parameter,\n    avg(ref_lai_target) as target_value,\n    avg(leaf_area_index_lai) as actual_value,\n    'm¬≤/m¬≤' as unit\n  FROM vertiflow.basil_ultimate_realtime\n  WHERE timestamp > now() - INTERVAL 1 HOUR\n  UNION ALL\n  SELECT\n    'Essential Oil' as parameter,\n    avg(ref_oil_target) as target_value,\n    avg(essential_oil_yield) as actual_value,\n    '%' as unit\n  FROM vertiflow.basil_ultimate_realtime\n  WHERE timestamp > now() - INTERVAL 1 HOUR\n  UNION ALL\n  SELECT\n    'Water Use Efficiency' as parameter,\n    avg(ref_wue_target) as target_value,\n    avg(wue_current) as actual_value,\n    'g/L' as unit\n  FROM vertiflow.basil_ultimate_realtime\n  WHERE timestamp > now() - INTERVAL 1 HOUR\n)\nSELECT\n  parameter,\n  round(target_value, 2) as target_value,\n  round(actual_value, 2) as actual_value,\n  unit,\n  round(abs(actual_value - target_value) / target_value * 100, 2) as gap_pct,\n  CASE\n    WHEN abs(actual_value - target_value) / target_value * 100 < 5 THEN 'OK'\n    WHEN abs(actual_value - target_value) / target_value * 100 < 10 THEN 'WARNING'\n    ELSE 'CRITICAL'\n  END as status\nFROM gaps\nORDER BY gap_pct DESC",
          "refId": "A"
        }
      ],
